<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0"
  xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <!-- If you create an app based off this repo, create an own ID e.g., via https://www.guidgen.com -->
  <Id>83025e38-b768-4a12-841e-48f04976db2c</Id>
  <Version>1.0.0</Version>
  <ProviderName>DataLink</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="FogLAMP Data Link" />
  <Description DefaultValue="A FogLAMP Data Link App." />
  <IconUrl DefaultValue="https://mr901.github.io/excel-officejs/icons/icon-32.png" />
  <HighResolutionIconUrl DefaultValue="https://mr901.github.io/excel-officejs/icons/icon-64.png" />
  <SupportUrl DefaultValue="https://dianomic.com/contact-us" />
  <AppDomains>
    <AppDomain>https://mr901.github.io/excel-officejs/</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Workbook" />
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://mr901.github.io/excel-officejs/taskpane.html" />
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Workbook">
        <DesktopFormFactor>
          <FunctionFile resid="FunctionFile.Url" />
          <!-- resid must point to a Url Resource -->
          <ExtensionPoint xsi:type="PrimaryCommandSurface">

            <CustomTab id="MyTab">
              <Group id="MyCommandsGroup">
                <Label resid="MyCommandsGroup.Label" />
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16" />
                  <bt:Image size="32" resid="Icon.32x32" />
                  <bt:Image size="80" resid="Icon.80x80" />
                </Icon>

                <!-- Ribbon button that opens the task pane -->
                <Control xsi:type="Button" id="MyTaskpaneButton">
                  <Label resid="MyTaskpaneButton.Label" />
                  <Supertip>
                    <Title resid="MyTaskpaneButton.Label" />
                    <Description resid="MyTaskpaneButton.Tooltip" />
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16" />
                    <bt:Image size="32" resid="Icon.32x32" />
                    <bt:Image size="80" resid="Icon.80x80" />
                  </Icon>
                  <!-- Action type must be ShowTaskpane -->
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>ButtonId1</TaskpaneId>
                    <!-- resid must point to a Url Resource -->
                    <SourceLocation resid="Taskpane.Url" />
                  </Action>
                </Control>

              </Group>
              <Group id="HelpGroup">
                <Label resid="HelpGroup.Label" />
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16" />
                  <bt:Image size="32" resid="Icon.32x32" />
                  <bt:Image size="80" resid="Icon.80x80" />
                </Icon>

                <Control xsi:type="Button" id="DocsButton">
                  <Label resid="DocsButton.Label" />
                  <Supertip>
                    <Title resid="DocsButton.Label" />
                    <Description resid="DocsButton.Tooltip" />
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16" />
                    <bt:Image size="32" resid="Icon.32x32" />
                    <bt:Image size="80" resid="Icon.80x80" />
                  </Icon>
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>openDocumentation</FunctionName>
                  </Action>
                </Control>

                <Control xsi:type="Button" id="ContactButton">
                  <Label resid="ContactButton.Label" />
                  <Supertip>
                    <Title resid="ContactButton.Label" />
                    <Description resid="ContactButton.Tooltip" />
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16" />
                    <bt:Image size="32" resid="Icon.32x32" />
                    <bt:Image size="80" resid="Icon.80x80" />
                  </Icon>
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>openContact</FunctionName>
                  </Action>
                </Control>

              </Group>
              <Label resid="MyTab.TabLabel" />
            </CustomTab>

          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>

    <Resources>
      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="https://mr901.github.io/excel-officejs/icons/icon-16.png" />
        <bt:Image id="Icon.32x32" DefaultValue="https://mr901.github.io/excel-officejs/icons/icon-32.png" />
        <bt:Image id="Icon.80x80" DefaultValue="https://mr901.github.io/excel-officejs/icons/icon-80.png" />
      </bt:Images>
      <bt:Urls>
        <!-- This must point to the HTML document with the task pane -->
        <bt:Url id="Taskpane.Url" DefaultValue="https://mr901.github.io/excel-officejs/taskpane.html" />
        <bt:Url id="FunctionFile.Url" DefaultValue="https://mr901.github.io/excel-officejs/commands.html" />
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="MyTab.TabLabel" DefaultValue="DataLink" />
        <bt:String id="MyCommandsGroup.Label" DefaultValue="MyGroup" />
        <bt:String id="MyTaskpaneButton.Label" DefaultValue="Show Taskpane" />
        <bt:String id="HelpGroup.Label" DefaultValue="Help & Links" />
        <bt:String id="DocsButton.Label" DefaultValue="Documentation" />
        <bt:String id="ContactButton.Label" DefaultValue="Contact Us" />
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="MyTaskpaneButton.Tooltip" DefaultValue="Click to show the task pane" />
        <bt:String id="DocsButton.Tooltip" DefaultValue="Open FogLAMP documentation in your browser" />
        <bt:String id="ContactButton.Tooltip" DefaultValue="Open Dianomic contact page in your browser" />
      </bt:LongStrings>
    </Resources>

  </VersionOverrides>
</OfficeApp>
